{
  "expo": {
    "name": "Octo",
    "slug": "octo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d1117"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.octo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d1117"
      },
      "package": "com.octo.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/GeistSans-Regular.otf",
            "./assets/fonts/GeistSans-Medium.otf",
            "./assets/fonts/GeistSans-SemiBold.otf",
            "./assets/fonts/GeistSans-Bold.otf",
            "./assets/fonts/GeistMono-Regular.otf",
            "./assets/fonts/GeistMono-Medium.otf",
            "./assets/fonts/GeistMono-SemiBold.otf",
            "./assets/fonts/GeistMono-Bold.otf"
          ]
        }
      ]
    ],
    "scheme": "octo",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
